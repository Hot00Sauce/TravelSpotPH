{
    "functions": {
        "*.php": {
            "runtime": "vercel-php@0.6.1"
        }
    },
    "routes": [
        {
            "src": "/",
            "dest": "/home.php"
        },
        {
            "src": "/(.*\\.php)",
            "dest": "/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}
